//Max, Min of an array

let a=[45,23,53,11,12,9,7,1,25]
let max=a[0]
let min=a[0]
for(let i=0;i<a.length;i++){
  if(a[i]>max){
    max=a[i]
  }
  if(a[i]<min){
    min=a[i]
  }
}
console.log("Max of the array is",max)
console.log("Min of the array is",min)


//Pattern program

let n = 5;

//Count number of character in a string

let a = "shreeprabha";
let count = {};

for (let i = 0; i < a.length; i++) {
  if (count[a[i]]) {
    count[a[i]]++;
  } else {
    count[a[i]] = 1;
  }
}

for (let char in count) {
  console.log(char, "=", count[char]);
}
